<Window x:Class="SudokuSolver.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SudokuSolver"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
  <Grid>
    <TextBox x:Name="Output" HorizontalAlignment="Left" Height="148" Margin="285,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="211"/>
    <TextBox x:Name="Input" HorizontalAlignment="Left" Height="148" Margin="19,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="188"/>
    <Label x:Name="label" Content="Input" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="19,14,0,0" Width="77"/>
    <Label x:Name="label_Copy" Content="Output" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="285,14,0,0" Width="77"/>
    <Label x:Name="label1" Content="Data:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="19,234,0,0"/>
    <TextBlock x:Name="Statistics" HorizontalAlignment="Left" Margin="73,239,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="423"><Run Text="Statistics"/><LineBreak/><Run/></TextBlock>
    <Button x:Name="button" Content="Open" HorizontalAlignment="Left" Margin="19,271,0,0" VerticalAlignment="Top" Width="75" Click="OpenFile"/>

  </Grid>
</Window>
